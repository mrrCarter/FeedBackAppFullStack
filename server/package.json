  "dependencies": {
    "@types/express": "^4.16.0",
    "cookie-session": "^2.0.0-beta.3",
    "eslint": "^5.9.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.11",
    "nodemon": "^1.18.6",
    "passport": "~0.4.0",
    "passport-google-oauth20": "~1.0.0"
